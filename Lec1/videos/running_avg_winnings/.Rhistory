cols = c(rgb(0.3,1,0.3),rgb(1,1,0.3),rgb(1,0.3,0.3))
frequencies = c(0,0,0)
for(ii in 1:length(phits)){
xx = 0.1+0.1*((ii-1)%%8)
yy = 0.9-0.1*((ii-1)%/%8)
